<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="eXam.ReviewPage">
  <ListView x:name="listQuestions" ItemTapped="itemTapped" HasUnevenRows="True" ItemsSource="QuestionViewModels" >
        <ListView.ItemTemplate>
      <DataTemplate>
        <ViewCell>
          <StackLayout>
            <Label Text="{Binding Question}"></Label>
             <Label Text="{Binding Answer}"></Label>
             <Label Text="{Binding Response}"></Label>
         </StackLayout>
        </ViewCell>
      </DataTemplate>
    </ListView.ItemTemplate>

  </ListView>
</ContentPage>